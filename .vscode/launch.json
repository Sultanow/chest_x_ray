{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Debug jupyter",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/backend/env/jupyter/bin/python"
    },
    {
      "name": "Stage1",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/backend/stage1/app.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/backend/env/chest_x_ray/bin/python",
      "cwd": "${workspaceFolder}/backend/stage1"
    },
    {
      "name": "Stage2",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/backend/stage2/app.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/backend/env/chest_x_ray/bin/python",
      "cwd": "${workspaceFolder}/backend/stage2"
    },
    {
      "name": "Stage3",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/backend/stage3/app.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/backend/env/chest_x_ray/bin/python",
      "cwd": "${workspaceFolder}/backend/stage3"
    },
    {
      "type": "firefox",
      "request": "launch",
      "name": "Angular",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/frontend",
    }
  ]
}